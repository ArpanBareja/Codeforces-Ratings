{
    "manifest_version": 3,
    "name": "Codeforces Ratings",
    "description": "A browser extension used to know your friends' ratings on Codeforces",
    "version": "0.0.1",
    "action": {
        "default_title": "Codeforces-Ratings",
        "default_icon": "CFR.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"],
    "icons": {
        "128": "CFR.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ]
}
